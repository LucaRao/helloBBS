.swiper-tab-item {  
  color: rgb(223, 217, 217); 
  display: inline-block; 
   text-align: center; 
   font-size: 27rpx;
   width: 100rpx;
  }
  .swiper-tab-item-content{
    color: rgb(31, 30, 30); 
    display: inline-block; 
     text-align: center; 
     font-size: 27rpx;
     width: 16%;
  }
  .swiper-tab {  
    display: flex;
    justify-content: space-around;
   line-height: 80rpx; 
   white-space: nowrap; 
   overflow: auto;
   background-color: #2a3886;
  }

  .swiper-tab-content{
    display: flex;
    justify-content: space-around;
   line-height: 85rpx; 
   white-space: nowrap; 
   overflow: auto;
   background-color: #eee;
  }
  .active { 
   display: inline-block; 
   color: #fff; 
   font-weight: bold;
  border-bottom: 4rpx solid #fff; 
   text-align: center; 
   font-size: 27rpx;
   width: 100rpx;
  }
  .active-content{
    color: #2a3886;
    font-weight: bold;
    width: 16%;
    text-align: center;
    border-top-right-radius: 12rpx;
    border-top-left-radius: 12rpx;
    background-color: #fff;
  }
